#include <iostream>

//using namespace std;


int main(void){
    
    double a = 3.4;
    double b(3.4);
    //auto b = (3.4);  x
    const int ci = 2;
    constexpr double cd = 1.4 * 5;



    std::cout<<"a:"<<a<<"b:"<<b<<"\n";
    std::cout<<"---------------"<<"\n";
    std::cout<<"ci:"<<ci<<"cd:"<<cd<<"\n";
    return 0;

}
